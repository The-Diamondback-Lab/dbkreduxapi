{
    "version": 2,
    "name": "dbk-api",
    "builds": [
        { "src": "doc/*.html", "use": "@now/static" },
        { "src": "*.css", "use": "@now/static" },
        { "src": "**/*.js", "use": "@now/node"}
    ],
    "routes": [
        { "src": "/", "dest": "doc/index.html"},
        { "src": "/(.*)", "dest": "app.js" }
    ],
    "alias": "dbk-api-production.now.sh"
}